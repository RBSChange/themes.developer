<?xml version="1.0" encoding="UTF-8"?>
<change:templates xmlns:change="http://www.rbs.fr/change/1.0/schema">
	<change:template id="onecolumn">
		<div id="top" orient="vertical">
			<div id="tools" orient="vertical">
				<div id="mainMenu">
					<change:templateblock type="modules_website_ConfigurableMenu" editname="mainmenu" __mode="tag" __tag="contextual_website_website_menu-main" __displayMode="default" __depth="1" />
				</div>
				<div id="threadandsearch" orient="horizontal">
					<div id="search" orient="horizontal">
						<change:templateblock type="modules_solrsearch_form" editname="searchform" />
					</div>
					<div id="languages" orient="horizontal">
						<change:templateblock type="modules_website_switchlanguage" editname="switchlangage" __viewall="false" __showflag=""/>
					</div>
					<div id="thread" orient="vertical">
						<change:templateblock type="modules_website_thread" editname="thread" />
					</div>
				</div>
			</div>
			<div id="contentLocation" orient="horizontal">
				<div id="sidebar" orient="vertical">
					<div id="contextual-menu">
						<change:templateblock type="modules_website_ConfigurableMenu" editname="contextualmenu" __mode="contextual" __displayMode="contextual" __deployAll="false" __showTitle="false" __depth="2" __startLevel="1"/>
					</div>
					<change:content id="sidebar-block"/>
				</div>
				<div id="content" orient="vertical">
					<change:content id="content-block" />
				</div>
			</div>
		</div>
	</change:template>
	<change:template id="top" content-type="xul" orient="vertical">
		<div id="contentLocation" orient="horizontal">
			<div id="content" orient="vertical">
				<change:content id="content-block" />
			</div>
			<div id="sidebar" orient="vertical">
				<change:content id="sidebar-block"/>
			</div>
		</div>
	</change:template>
</change:templates>