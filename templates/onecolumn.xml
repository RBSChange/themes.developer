<?xml version="1.0" encoding="UTF-8"?>
<change:templates xmlns:change="http://www.rbs.fr/change/1.0/schema">
	<change:template id="onecolumn">
		<div id="top" orient="vertical">
			<div id="tools" orient="vertical">
				<div id="mainMenu">
					<change:templateblock editname="menu" type="modules_website_taggedmenu" __tag="contextual_website_website_menu-main" __template="horizontal" __depth="0" />
				</div>
				<div id="threadandsearch" orient="horizontal">
					<div id="search" orient="horizontal">
						<change:templateblock editname="search" type="modules_solrsearch_form" />
					</div>
					<div id="languages" orient="horizontal">
						<change:templateblock editname="language" type="modules_website_switchlanguage" __viewall="false" __showflag=""/>
					</div>
					<div id="thread" orient="vertical">
						<change:templateblock type="modules_website_thread" />
					</div>
				</div>
			</div>
			<div id="tools2" orient="vertical">
				<div id="mainMenu2">
					<change:templateblock editname="menu2" type="modules_website_taggedmenu" __tag="contextual_website_website_menu-main" __template="horizontal" __depth="0" />
				</div>
				<div id="threadandsearch2" orient="horizontal">
					<div id="search2" orient="horizontal">
						<change:templateblock editname="search2" type="modules_solrsearch_form" />
					</div>
					<div id="languages2" orient="horizontal">
						<change:templateblock editname="language2" type="modules_website_switchlanguage" __viewall="false" __showflag=""/>
					</div>
					<div id="thread2" orient="vertical">
						<change:templateblock type="modules_website_thread" />
					</div>
				</div>
			</div>
			<div id="contentLocation" orient="horizontal">
				<div id="sidebar" orient="vertical">
					<div id="contextual-menu">
						<change:templateblock editname="contextmenu" type="modules_website_contextualmenu" __template="vertical" __depth="0" __startwebsitelevel="2"/>
					</div>
					<change:content id="sidebar-block"/>
				</div>
				<div id="content" orient="vertical">
					<change:content id="content-block" />
				</div>
			</div>
		</div>
	</change:template>
	<change:template id="top" content-type="xul" orient="vertical">
		<div id="contentLocation" orient="horizontal">
			<div id="content" orient="vertical">
				<change:content id="content-block" />
			</div>
			<div id="sidebar" orient="vertical">
				<change:content id="sidebar-block"/>
			</div>
		</div>
	</change:template>
</change:templates>